<head runat="server">
    <title></title>

    <!DOCTYPE link href="https://maps/documentation/javascript/examples/default.css" rel="stylesheet">
    <html>
    @section scripts {
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDrZOEE4toOw4gpRD6pNVgyp1quc2Da8JQ"type="text/javascript"></script>
        <script>

            var mapcode;
            var diag;

            function initialize() {
                mapcode = new window.google.maps.Geocoder();
                var lnt = new window.google.maps.LatLng(59.875679, 29.825597);
                var diagChoice = {
                    zoom: 19,
                    center: lnt,
                    diagId: window.google.maps.MapTypeId.ROADMAP
                }
                diag = new window.google.maps.Map(document.getElementById('map_populate'), diagChoice);
            }

            google.maps.event.addDomListener(window, 'load', initialize);
        </script>
    }

    </html>
</head>

<form id="form1" runat="server">
    <div>
